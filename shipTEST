import table

class Ship:

    def create(self, table, width=10, height=75, x_pos=50,
               y_pos=50, color="blue", x_speed=35, y_speed=30):
        self.width = width
        self.height = height
        self.x_pos = x_pos
        self.y_pos = y_pos
        self.color = color
        self.x_start = x_pos
        self.y_start = y_pos
        self.x_speed = x_speed
        self.y_speed = y_speed
        self.table = table
        self.rectangle = self.table.draw_rectangle(self)

        

